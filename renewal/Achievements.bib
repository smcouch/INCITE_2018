  Doi		= {10.1051/0004-6361/201117611},
  Eid		= {A63},
  Eprint	= {1106.6301},
  Journal	= {\aap},
  Keywords	= {stars: neutron, hydrodynamics, neutrinos, stars: massive,
		  supernovae: general, gravitational waves},
  Month		= Jan,
  Pages		= {A63},
  Primaryclass	= {astro-ph.SR},
  Title		= {{Parametrized 3D models of neutrino-driven supernova
		  explosions. Neutrino emission asymmetries and
		  gravitational-wave signals}},
