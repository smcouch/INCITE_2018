  Bdsk-Url-3	= {http://adsabs.harvard.edu/abs/2013JCoPh.243..269L}
}

@Article{	  roberts:2016,
  Abstract	= {We report on a set of long-term general-relativistic
		  three-dimensional (3D) multi-group (energy-dependent)
		  neutrino radiation-hydrodynamics simulations of
		  core-collapse supernovae. We employ a full 3D two-moment
		  scheme with the local M1 closure, three neutrino species,
