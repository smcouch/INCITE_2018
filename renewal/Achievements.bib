  Author	= {{Hanke}, F. and {M{\"u}ller}, B. and {Wongwathanarat}, A.
		  and {Marek}, A. and {Janka}, H.-T.},
  Date-Added	= {2017-08-10 14:39:15 +0000},
  Date-Modified	= {2017-12-05 16:36:32 +0000},
  Doi		= {10.1088/0004-637X/770/1/66},
  Eid		= {66},
