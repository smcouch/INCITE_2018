  Date-Added	= {2017-08-10 13:59:22 +0000},
  Date-Modified	= {2017-12-05 16:37:05 +0000},
  Doi		= {10.1016/j.jcp.2007.11.038},
  Journal	= {Journal of Computational Physics},
  Month		= Mar,
  Pages		= {3191-3211},
  Title		= {{An improved weighted essentially non-oscillatory scheme
		  for hyperbolic conservation laws}},
