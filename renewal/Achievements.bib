  Eprint	= {astro-ph/0501557},
  Journal	= {Journal of Computational Physics},
  Month		= May,
  Pages		= {509-539},
  Title		= {{An unsplit Godunov method for ideal MHD via constrained
		  transport}},
  Volume	= 205,
  Year		= 2005,
  Bdsk-File-1	= {},
  Bdsk-Url-1	= {http://dx.doi.org/10.1016/j.jcp.2004.11.016},
