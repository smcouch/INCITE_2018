}

@Article{	  ott:2008,
  Abstract	= {We perform axisymmetric (2D) multiangle, multigroup
		  neutrino radiation-hydrodynamic calculations of the
		  postbounce phase of core-collapse supernovae using a
		  genuinely 2D discrete-ordinate (Sn) method. We follow the
		  long-term postbounce evolution of the cores of one
		  nonrotating and one rapidly rotating 20 M&sun; stellar
		  model for ~400 milliseconds from 160 to ~550 ms after
		  bounce. We present a multidimensional analysis of the
		  multiangle neutrino radiation fields and compare in detail
		  with counterpart simulations carried out in the 2D
		  multigroup flux-limited diffusion (MGFLD) approximation to
		  neutrino transport. We find that 2D multiangle transport is
		  superior in capturing the global and local radiation-field
		  variations associated with rotation-induced and
		  SASI-induced aspherical hydrodynamic configurations. In the
		  rotating model, multiangle transport predicts much larger
		  asymptotic neutrino flux asymmetries with pole-to-equator
		  ratios of up to ~2.5, while MGFLD tends to sphericize the
		  radiation fields already in the optically semitransparent
		  postshock regions. Along the poles, the multiangle
		  calculation predicts a dramatic enhancement of the neutrino
		  heating by up to a factor of 3, which alters the postbounce
		  evolution and results in greater polar shock radii and an
		  earlier onset of the initially rotationally weakened SASI.
		  In the nonrotating model, differences between multiangle
		  and MGFLD calculations remain small at early times when the
		  postshock region does not depart significantly from
		  spherical symmetry. At later times, however, the growing
		  SASI leads to large-scale asymmetries and the multiangle
		  calculation predicts up to 30% higher average integral
		  neutrino energy deposition rates than MGFLD.},
  Adsnote	= {Provided by the SAO/NASA Astrophysics Data System},
  Adsurl	= {http://adsabs.harvard.edu/abs/2008ApJ...685.1069O},
  Archiveprefix	= {arXiv},
  Arxivurl	= {http://arxiv.org/abs/0804.0239},
  Author	= {{Ott}, C.~D. and {Burrows}, A. and {Dessart}, L. and
		  {Livne}, E.},
  Date-Added	= {2017-08-10 16:19:44 +0000},
  Date-Modified	= {2017-12-05 16:37:00 +0000},
  Doi		= {10.1086/591440},
  Eid		= {1069-1088},
  Eprint	= {0804.0239},
  Journal	= {\apj},
  Keywords	= {hydrodynamics, neutrinos, radiative transfer, stars:
		  evolution, stars: neutron, supernovae: general},
  Month		= Oct,
  Pages		= {1069-1088},
  Title		= {{Two-Dimensional Multiangle, Multigroup Neutrino
		  Radiation-Hydrodynamic Simulations of Postbounce Supernova
		  Cores}},
  Volume	= 685,
  Year		= 2008,
  Bdsk-File-1	= {},
