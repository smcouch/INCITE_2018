  Doi		= {10.1086/317361},
  Journal	= {\apjs},
  Keywords	= {Equation of State, Hydrodynamics, Methods: Numerical,
		  Nuclear Reactions, Nucleosynthesis, Abundances, Stars:
		  General},
  Month		= Nov,
