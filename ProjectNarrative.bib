  Doi		= {10.1146/annurev-nucl-102115-044747},
  Eprint	= {1602.05576},
  Journal	= {Annual Review of Nuclear and Particle Science},
  Keywords	= {Astrophysics - Solar and Stellar Astrophysics, High Energy
		  Physics - Phenomenology, Nuclear Theory},
  Month		= Oct,
  Pages		= {341-375},
  Primaryclass	= {astro-ph.SR},
  Title		= {{Physics of Core-Collapse Supernovae in Three Dimensions:
		  A Sneak Preview}},
  Volume	= 66,
  Year		= 2016,
  Bdsk-File-1	= {},
  Bdsk-Url-1	= {http://dx.doi.org/10.1146/annurev-nucl-102115-044747},
