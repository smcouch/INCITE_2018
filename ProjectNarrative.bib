  Date-Modified	= {2015-07-22 15:23:11 +0000},
  Doi		= {10.1088/0004-637X/775/1/35},
  Eid		= {35},
  Eprint	= {1212.0010},
  Journal	= {\apj},
  Keywords	= {hydrodynamics, neutrinos, stars: interiors, supernovae:
		  general},
  Month		= Sep,
  Pages		= {35},
  Primaryclass	= {astro-ph.HE},
  Title		= {{On the Impact of Three Dimensions in Simulations of
		  Neutrino-driven Core-collapse Supernova Explosions}},
