  Eid		= {014046},
  Journal	= {Physica Scripta Volume T},
  Month		= Dec,
  Number	= 1,
  Pages		= {014046},
  Title		= {{Introduction to FLASH 3.0, with application to supersonic
		  turbulence}},
  Volume	= 132,
