  Author	= {{Melson}, T. and {Janka}, H.-T. and {Bollig}, R. and
		  {Hanke}, F. and {Marek}, A. and {M{\"u}ller}, B.},
  Date-Added	= {2015-09-08 15:16:15 +0000},
  Date-Modified	= {2015-11-02 13:47:29 +0000},
  Doi		= {10.1088/2041-8205/808/2/L42},
  Eid		= {L42},
  Eprint	= {1504.07631},
  Journal	= {\apjl},
  Keywords	= {hydrodynamics, instabilities, neutrinos, supernovae:
		  general},
  Month		= Aug,
  Pages		= {L42},
  Primaryclass	= {astro-ph.SR},
  Title		= {{Neutrino-driven Explosion of a 20 Solar-mass Star in
		  Three Dimensions Enabled by Strange-quark Contributions to
		  Neutrino-Nucleon Scattering}},
  Volume	= 808,
  Year		= 2015,
  Bdsk-Url-1	= {http://dx.doi.org/10.1088/2041-8205/808/2/L42},
