  Date-Added	= {2015-07-18 15:19:35 +0000},
  Date-Modified	= {2015-07-18 15:19:36 +0000},
  Doi		= {10.1088/0264-9381/27/11/114103},
  Eid		= {114103},
  Eprint	= {0912.2393},
  Journal	= {Classical and Quantum Gravity},
  Month		= Jun,
  Number	= 11,
  Pages		= {114103},
  Primaryclass	= {astro-ph.HE},
  Title		= {{A new open-source code for spherically symmetric stellar
		  collapse to neutron stars and black holes}},
