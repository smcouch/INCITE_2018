  Doi		= {10.1016/S0010-4655(99)00501-9},
  Journal	= {Computer Physics Communications},
  Month		= Apr,
  Pages		= {330-354},
  Title		= {{PARAMESH: A parallel adaptive mesh refinement community
		  toolkit}},
  Volume	= 126,
  Year		= 2000,
  Bdsk-Url-1	= {http://dx.doi.org/10.1016/S0010-4655(99)00501-9},
