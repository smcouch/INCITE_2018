  Doi		= {10.1088/0004-637X/765/1/29},
  Eid		= {29},
  Eprint	= {1206.4724},
  Journal	= {\apj},
  Keywords	= {hydrodynamics, methods: numerical, shock waves, stars:
		  interiors, supernovae: general},
  Month		= Mar,
  Pages		= {29},
  Primaryclass	= {astro-ph.HE},
  Title		= {{The Dependence of the Neutrino Mechanism of Core-collapse
		  Supernovae on the Equation of State}},
  Volume	= 765,
  Year		= 2013,
  Bdsk-Url-1	= {http://dx.doi.org/10.1088/0004-637X/765/1/29},
