  Keywords	= {hydrodynamics, neutrinos, stars: interiors, supernovae:
		  general},
  Month		= Mar,
  Pages		= {110},
  Primaryclass	= {astro-ph.SR},
  Title		= {{Dimensional Dependence of the Hydrodynamics of
		  Core-collapse Supernovae}},
