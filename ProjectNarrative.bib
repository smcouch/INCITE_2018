  Eprint	= {astro-ph/9511106},
  Journal	= {Physical Review Letters},
  Month		= Jan,
  Pages		= {352-355},
  Title		= {{Pulsar Recoil and Gravitational Radiation Due to
		  Asymmetrical Stellar Collapse and Explosion}},
