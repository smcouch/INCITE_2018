
@Article{	  abbott:2017,
  Abstract	= {We describe the observation of GW170104, a
		  gravitational-wave signal produced by the coalescence of a
		  pair of stellar-mass black holes. The signal was measured
		  on January 4, 2017 at 10:11:58.6 UTC by the twin advanced
		  detectors of the Laser Interferometer Gravitational-Wave
		  Observatory during their second observing run, with a
		  network signal-to-noise ratio of 13 and a false alarm rate
		  less than 1 in 70 000 years. The inferred component black
		  hole masses are 31. 2-6.0+8.4M&sun; and 19. 4-5.9+5.3
		  M&sun; (at the 90% credible level). The black hole spins
		  are best constrained through measurement of the effective
		  inspiral spin parameter, a mass-weighted combination of the
		  spin components perpendicular to the orbital plane,
		  chieff=-0.1 2-0.30+0.21 . This result implies that spin
		  configurations with both component spins positively aligned
		  with the orbital angular momentum are disfavored. The
		  source luminosity distance is 88 0-390+450 Mpc
		  corresponding to a redshift of z =0.1 8-0.07+0.08 . We
		  constrain the magnitude of modifications to the
		  gravitational-wave dispersion relation and perform null
		  tests of general relativity. Assuming that gravitons are
		  dispersed in vacuum like massive particles, we bound the
		  graviton mass to mg<=7.7 ×10-23 eV /c2 . In all cases, we
		  find that GW170104 is consistent with general relativity.},
  Adsnote	= {Provided by the SAO/NASA Astrophysics Data System},
  Adsurl	= {http://adsabs.harvard.edu/abs/2017PhRvL.118v1101A},
  Author	= {{Abbott}, B.~P. and {Abbott}, R. and {Abbott}, T.~D. and
		  {Acernese}, F. and {Ackley}, K. and {Adams}, C. and
		  {Adams}, T. and {Addesso}, P. and {Adhikari}, R.~X. and
		  {Adya}, V.~B. and et al.},
  Date-Added	= {2017-06-20 13:18:34 +0000},
  Date-Modified	= {2017-06-20 13:18:36 +0000},
  Doi		= {10.1103/PhysRevLett.118.221101},
  Eid		= {221101},
  Journal	= {Physical Review Letters},
  Month		= Jun,
  Number	= 22,
  Pages		= {221101},
  Title		= {{GW170104: Observation of a 50-Solar-Mass Binary Black
		  Hole Coalescence at Redshift 0.2}},
  Volume	= 118,
  Year		= 2017,
  Bdsk-Url-1	= {http://dx.doi.org/10.1103/PhysRevLett.118.221101},
  Bdsk-Url-2	= {http://adsabs.harvard.edu/abs/2017PhRvL.118v1101A}
