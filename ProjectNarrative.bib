  Date-Added	= {2015-07-18 16:22:01 +0000},
  Date-Modified	= {2015-07-18 16:22:02 +0000},
  Doi		= {10.1088/0004-637X/703/2/1464},
  Eprint	= {0812.4574},
  Journal	= {\apj},
  Keywords	= {hydrodynamics, instabilities, nuclear reactions,
		  nucleosynthesis, abundances, shock waves, supernovae:
		  general},
  Month		= Oct,
  Pages		= {1464-1485},
  Title		= {{Dynamics of a Spherical Accretion Shock with Neutrino
		  Heating and Alpha-Particle Recombination}},
  Volume	= 703,
  Year		= 2009,
