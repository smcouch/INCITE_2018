  Doi		= {10.1016/j.physrep.2007.02.009},
  Eprint	= {astro-ph/0702176},
  Journal	= {\physrep},
  Month		= Apr,
  Pages		= {269-283},
  Title		= {{Nucleosynthesis and remnants in massive stars of solar
		  metallicity}},
  Volume	= 442,
  Year		= 2007,
  Bdsk-Url-1	= {http://dx.doi.org/10.1016/j.physrep.2007.02.009},
  Bdsk-Url-2	= {http://arXiv.org/abs/astro-ph/0702176},
