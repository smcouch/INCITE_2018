  Keywords	= {methods: numerical, stars: interiors, stars: massive,
		  supernovae: general, supernovae: individual: progenitors},
  Month		= Nov,
  Pages		= {92},
  Title		= {{Characterizing the Convective Velocity Fields in Massive
		  Stars}},
  Volume	= 795,
  Year		= 2014,
  Bdsk-File-1	= {},
