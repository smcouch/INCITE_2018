  Doi		= {10.1086/338953},
  Eprint	= {astro-ph/0112020},
  Journal	= {\apj},
  Keywords	= {Gamma Rays: Bursts, ISM: Jets and Outflows, Stars:
		  Pulsars: General, Stars: Supernovae: General},
  Month		= Apr,
  Pages		= {807-819},
  Title		= {{Asymmetric Supernovae from Magnetocentrifugal Jets}},
  Volume	= 568,
  Year		= 2002,
