		  and {Marek}, A. and {Janka}, H.-T.},
  Date-Added	= {2015-07-18 16:25:33 +0000},
  Date-Modified	= {2015-07-18 16:25:34 +0000},
  Doi		= {10.1088/0004-637X/770/1/66},
  Eid		= {66},
